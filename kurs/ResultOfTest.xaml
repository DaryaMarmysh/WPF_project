
<Page x:Class="kurs.ResultOfTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:tool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        xmlns:dv="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"       
        
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        Title="MainWindow" Height="350" Width="525"
        >
    <Grid>
        <dv:Chart  Name="pieChart" Title="" VerticalAlignment="Top" Height="340"  BorderBrush="{x:Null}" Padding="0" Margin="0,0,-195.2,0">
            <dv:PieSeries DependentValuePath="Value" IndependentValuePath="Key" ItemsSource="{Binding}" IsSelectionEnabled="True" Margin="10,10,-194.8,9.6"  />
            </dv:Chart>
        <Button x:Name="buttonLeft" Content="l" HorizontalAlignment="Left" Margin="197,287,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="buttonRight" Content="r" HorizontalAlignment="Left" Margin="440,287,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>

</Page>